query GetEvents($deviceId:String){
    blsEvents(deviceId:$deviceId){
      deviceId,
      event,
      eventtime,
          latitude,
      longitude,
          workerId,
          workerName
    }
  }
  //Variables
  {
    "deviceId":"4"
  }

